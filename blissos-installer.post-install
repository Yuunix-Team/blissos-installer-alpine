#!/bin/bash

export PATH=/bin:/sbin:/usr/local/bin:/usr/bin:/usr/sbin

cd /
mkdir -p \
	android \
	boot/grub \
	boot/efi \
	cdrom
	
for d in data \
	data_mirror \
	system \
	storage \
	sdcard \
	apex \
	linkerconfig \
	debug_ramdisk \
	system_ext \
	product \
	vendor; do ln -s /android/$d /$d; done

for d in modules firmware; do ln -s /system/lib/$d /lib/$d; done
